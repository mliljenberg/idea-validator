# Copy this file to `.env` and fill in real values.
# Required for this project:
GOOGLE_API_KEY=your_google_api_key_here
BRAVE_SEARCH_API_KEY=your_brave_search_api_key_here

# Optional: if you prefer this name instead of GOOGLE_API_KEY
# GEMINI_API_KEY=your_gemini_api_key_here

# Optional source/provider credentials
GITHUB_TOKEN=ghp_your_token_here
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here
REDDIT_USER_AGENT=product-validator/0.1

# Optional legacy/experimental integrations
PRODUCT_HUNT_TOKEN=your_product_hunt_token_here
CRUNCHBASE_API_KEY=your_crunchbase_api_key_here

# Optional model toggles (if your workflow uses them)
PV_ENABLE_LLM_ANALYSIS=true
PV_LLM_ANALYSIS_MODEL=gemini-3.0-flash
